#include<iostream>
using namespace std;
int main()
{
    int t;
    scanf("%d",&t);
    getchar();
    while(t>0)
    {
              char s1[500],s2[500];
              int a[26]={0},b[26]={0};
              
             gets(s1);
             gets(s2);
         //    cout<<s1<<endl;
          //   cout<<s2<<endl;
              int i=0;
             // cout<<"9090990"<<endl;
              while(s1[i]!='\0')
              {
                if(s1[i]>=65 && s1[i]<=90)
                s1[i]=s1[i]+32;
                  a[s1[i]-'a']++;
                  i++;
                                }
                                i=0;
              while(s2[i]!='\0')
              {
              if(s2[i]>=65 && s2[i]<=90)
                s2[i]=s2[i]+32;
                  b[s2[i]-'a']++;
                  i++;
                                }
                                int flag=0;
              for(int i=0;i<26;i++)
              {
                      if(a[i]!=b[i])
                      {
                             //       cout<<"a[i]="<<a[i]<<endl;
                               //     cout<<"b[i]="<<b[i]<<endl;
                                    if(a[i]>b[i])
                                    b[i]=b[i]-a[i];
                                    else
                                    a[i]=a[i]-b[i];
                                    
                                    flag=1;
                                 //   break;
                       }
              }
              /*
               for(int i=0;i<26;i++)
                       cout<<a[i]<<" ";
                       cout<<endl;
               for(int i=0;i<26;i++)
                       cout<<b[i]<<" ";
                       cout<<endl;
                       cout<<"upper"<<endl;
                       */
              if(flag==0)
              printf("YES");
              else
              {
                  int nega=0,negb=0;
                  for(int i=0;i<26;i++)
                  {
                          if(a[i]<0)
                          nega++;
                          if(b[i]<0)
                          negb++;
                  }
                  /*
                  cout<<"nega="<<nega<<endl;
                  cout<<"negb="<<negb<<endl;
                  */
                  if(nega>0 && negb>0)
                            printf("NO LUCK");
                  else if(nega>0)
                  {
                       int odd=0,hold=0;
                        for(int i=0;i<26;i++)
                            {
                                if(a[i]<0){
                                            a[i]=-a[i];
                                            if(a[i]%2!=0){
                                            odd++;
                                            hold=i;
                                            a[i]=a[i]-1;
                                            }}
                                else
                                a[i]=0;
                                }
                                
                            //    cout<<"--";
                            /*
                                for(int i=0;i<26;i++)
                                        cout<<a[i]<<" ";
                                        cout<<endl;
                                      */  
                          if(odd > 1)
                                  printf("NO LUCK");

                                else
                                {
                                    for(int i=0;    i<26;   i++)
                                    {
                                        for(int j=0;    j<a[i]/2;   j++)
                                            printf("%c",i+97);
                                    }
                    
                                    if(odd == 1)
                                        printf("%c",hold+97);
                    
                                    for(int i=25;  i>=0;    i--)
                                    {
                                        for(int j=0;    j<a[i]/2;   j++)
                                            printf("%c",i+97);
                                    }
                    
                                  //  printf("\n");
                                }
                  }
                  else
                  {
                       int odd=0,hold=0;
                        for(int i=0;i<26;i++)
                            {
                                if(b[i]<0){
                                        b[i]=-b[i];
                                        if(b[i]%2!=0){
                                        odd++;
                                        hold=i;
                                        b[i]=b[i]-1;}
                                }
                                else
                                b[i]=0;
                                }
                                 
                          if(odd > 1)
                                  printf("NO LUCK");

                                else
                                {
                                    for(int i=0;    i<26;   i++)
                                    {
                                        for(int j=0;    j<b[i]/2;   j++)
                                            printf("%c",i+97);
                                    }
                    
                                    if(odd == 1)
                                        printf("%c",hold+97);
                    
                                    for(int i=25;  i>=0;    i--)
                                    {
                                        for(int j=0;    j<b[i]/2;   j++)
                                            printf("%c",i+97);
                                    }
                    
                                  //  printf("\n");
                                }
                  }}
                  if(t!=1)
                  printf("\n");
              t--;
    }
    return 0;
}

