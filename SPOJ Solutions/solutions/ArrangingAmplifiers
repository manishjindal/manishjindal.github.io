#include<iostream>
using namespace std;


void merge(long long int arr[],long long int min,long long int mid,long long int max)
{
  long long int tmp[100005];
  long long int i,j,k,m; 
  j=min;
  m=mid+1;
  for(i=min; j<=mid && m<=max ; i++)
  {
     if(arr[j]<=arr[m])
     {
         tmp[i]=arr[j];
         j++;
     }
     else
     {
         tmp[i]=arr[m];
         m++;
     }
  }
  if(j>mid)
  {
     for(k=m; k<=max; k++)
     {
         tmp[i]=arr[k];
         i++;
     }
  }
  else
  {
     for(k=j; k<=mid; k++)
     {
        tmp[i]=arr[k];
        i++;
     }
  }
  for(k=min; k<=max; k++)
     arr[k]=tmp[k];
}

void part(long long int arr[],long long int min,long long int max)
{
 long long int mid;
 if(min<max)
 {
   mid=(min+max)/2;
   part(arr,min,mid);
   part(arr,mid+1,max);
   merge(arr,min,mid,max);
 }
}

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
              long long int a[100005];
              long long int n;
              scanf("%lld",&n);
              
              for(int i=0;i<n;i++)
              scanf("%lld",&a[i]);
    
              part(a,0,n-1);
    
              long long int i=0;
              while(a[i]==1){
              printf("%lld ",a[i]);
              i++;}
              
              
              if(a[n-1]<=3){
               for(int j=i;j<n;j++)
                      printf("%lld ",a[j]);
                      }
               else{
              for(int j=n-1;j>=i;j--)
              {
                      printf("%lld ",a[j]);
                      }
                   
                   }    
                      printf("\n");
    
    }
    return 0;
}

